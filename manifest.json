{
  "manifest_version": 2,
  "name": "Save to Kawpaa",
  "description": "あとで抜く",
  "version": "0.7.0",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "notifications",
    "http://*/*",
    "https://*/*",
    "chrome://favicon/"
  ],
  "content_scripts": [{
    "matches": [
      "https://twitter.com/*"
    ],
    "//": [
      "build/js/vendors/lib.js",
      "build/js/insertSaveButton2Twitter.js"
    ],
    "js": [
      "build/js/vendors/lib.min.js",
      "build/js/insertSaveButton2Twitter.min.js"
    ],
    "run_at": "document_end"
  }, {
    "matches": [
      "https://chan.sankakucomplex.com/*/post/show/*",
      "https://danbooru.donmai.us/posts/*",
      "http://*.deviantart.com/art/*",
      "http://gelbooru.com/index.php*",
      "https://konachan.com/post/show/*",
      "http://www.pixiv.net/member_illust.php*",
      "https://yande.re/post/show/*"
    ],
    "js": [
      "build/js/vendors/lib.js",
      "build/js/insertSaveLink.js"
    ],
    "//": [
      "build/js/vendors/lib.min.js",
      "build/js/insertSaveLink.min.js"
    ],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": {
      "19": "build/images/gray/icon19.png",
      "38": "build/images/gray/icon38.png"
    },
    "default_title": "Save to Kawpaa"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "build/js/vendors/lib.js",
      "build/js/background.js"
    ],
    "//": [
      "build/js/vendors/lib.min.js",
      "build/js/background.min.js"
    ]
  },
  "options_page": "build/views/views/options.html",
  "icons": {
    "16": "build/images/blue/icon19.png",
    "48": "build/images/blue/icon48.png",
    "128": "build/images/blue/icon128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "MacCtrl+Shift+H"
      }
    }
  }
}